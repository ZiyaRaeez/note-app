{
  "projects": {
    "note-taking-app": {
      "architect": {
        "build": {
          "options": {
            "baseHref": "/note-app/",
            "deployUrl": "/note-app/",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "src/public",
                "output": "/"
              }
            ],
            "styles": [
              "src/styles.scss",
              {
                "input": "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
                "inject": false
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        }
      }
    }
  }
}